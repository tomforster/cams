extends layout

block content
    #images.row
        each image in images
            .col-xl-2.col-lg-3.col-md-4.col-sm-6.mb-3
                a.card(href=baseUrl + image.file)
                    .overlay
                    img.img-fluid(src=baseUrl + image.file)
                    .date-time
                        .small= image.date
                        .small.text-right= image.time